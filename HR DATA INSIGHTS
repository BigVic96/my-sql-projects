--                         ----ANALYSIS--------
-- Employee & Workforce Basics
-- 1. How many employees are currently in the company?
select count(first_name) Total_employees from employees;

-- 2. Please provide a list of the top 10 highest-paid employees (name and salary).
select first_name,last_name, salary from employees 
group by first_name, last_name, salary 
order by first_name, last_name, salary desc limit 10;

-- 3. What are all the distinct job titles in the organization?
select distinct job_title from jobs;
select concat(first_name, ' ', last_name) from employees;
alter table employees add column Name text;
set sql_safe_updates=0;
update employees
set Name =concat(first_name, ' ', last_name);
select * from employees;
alter table employees drop column first_name;
alter table employees drop column last_name;
-- 4. How many employees are there in each department?
select d.department_name, count(Name) as employees from
employees e join departments d on e.department_id=d.department_id
group by d.department_name;

-- 5. Which department has the largest number of employees?
select d.department_name, count(e.name) employees from
employees e join departments d on e.department_id=d.department_id
group by d.department_name
order by employees desc limit 1;
/*with department_count as
( select d.department_name, count(e.Name) as employees from
employees e join departments d on e.department_id=d.department_id
group by d.department_name)
select department_name from department_count
order by employees desc limit 1;*/
-- 6. Which department has the highest average salary?
 select d.department_name, round(avg(e.salary),2) as avg_salary from
employees e join departments d on e.department_id=d.department_id
group by d.department_name
order by avg_salary desc limit 1;
-- or
with department_count as
( select d.department_name, round(avg(e.salary),2)as avg_salary from
employees e join departments d on e.department_id=d.department_id
group by d.department_name)
select department_name from department_count
order by avg_salary desc limit 1;

--     Salary & Compensation-------
-- 7. What is the minimum, maximum, and average salary across the company?
select max(salary) max_salary, min(salary) min_salary, 
round(avg(salary),2) avg_salary
from employees;

-- Who are the top 5 highest-paid employees?
select Name, salary
from employees
group by Name, salary
order by salary desc limit 5;
-- 9. How many employees earn more than $10,000, and can you list some examples?
select count(salary) earners 
from employees where salary > 10000
order by salary;

select name, salary from employees 
where salary > 10000 
group by name, salary
order by salary;

-- 10. What is the total annual payroll cost for each department?
select d.department_name, sum(e.salary) total_payroll from
employees e join departments d on e.department_id=d.department_id
group by department_name
order by total_payroll desc;
-- or 
with dept_name as
(select d.department_name, sum(e.salary) annaul_payroll 
from employees e join departments d on e.department_id=d.department_id
group by d.department_name)
select department_name from dept_name
order by annaul_payroll desc;
select round(avg(salary),2) from employees;
-- 11. Which employees earn above the company-wide average salary?
select Name from employees
where salary > 8060.00
order by salary desc;

-- 12. Are there any employees whose current salary exceeds the maximum defined for their job title?
select e.name, e.salary, j.job_title
from employees e join jobs j on e.job_id=j.job_id
where e.salary > j.max_salary
group by e.name, e.salary, j.job_title;


--     Hiring & Tenure
-- 13. Who is the most recently hired employee?
select Name, hire_date
from employees
order by hire_date desc limit 1;

/* 14. Who are the five longest-serving employees (earliest hire dates)?
Please present the results in a clear, readable format.*/
select Name, hire_date as longest_serving_employees
from employees
group by Name, hire_date
order by hire_date asc limit 5;

--  Additional
-- yrars_spent
select Name, hire_date,
timestampdiff(year,hire_date,curdate()) as yrs_spent
from employees;


