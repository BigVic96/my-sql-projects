 CREATE TABLE regions (
	region_id int PRIMARY KEY,
	region_name CHARACTER VARYING (25)
);
INSERT INTO regions(region_id,region_name) VALUES (1,'Europe');
INSERT INTO regions(region_id,region_name) VALUES (2,'Americas');
